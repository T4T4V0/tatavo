<svg width="48" height="48" viewBox="0 0 48 48" fill="none" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="gradient1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#6366f1;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8b5cf6;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="gradient2" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f8fafc;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e2e8f0;stop-opacity:1" />
    </linearGradient>
    <filter id="shadow" x="-50%" y="-50%" width="200%" height="200%">
      <feDropShadow dx="2" dy="3" stdDeviation="3" flood-color="#6366f1" flood-opacity="0.4"/>
    </filter>
  </defs>
  
  <!-- Background Circle -->
  <circle cx="24" cy="24" r="22" fill="url(#gradient1)" filter="url(#shadow)"/>
  
  <!-- 3D Cube representing development/tech -->
  <g transform="translate(24, 24)">
    <!-- Top face -->
    <path d="M-6,-9 L0,-12 L6,-9 L0,-6 Z" fill="url(#gradient2)" opacity="0.9"/>
    <!-- Left face -->
    <path d="M-6,-9 L-6,3 L0,6 L0,-6 Z" fill="url(#gradient2)" opacity="0.7"/>
    <!-- Right face -->
    <path d="M0,-6 L0,6 L6,3 L6,-9 Z" fill="url(#gradient2)" opacity="0.5"/>
  </g>
  
  <!-- Accent dots for tech/digital feel -->
  <circle cx="12" cy="12" r="1.5" fill="#ffffff" opacity="0.6"/>
  <circle cx="36" cy="12" r="1.5" fill="#ffffff" opacity="0.4"/>
  <circle cx="12" cy="36" r="1.5" fill="#ffffff" opacity="0.4"/>
  <circle cx="36" cy="36" r="1.5" fill="#ffffff" opacity="0.6"/>
  
  <!-- Additional tech elements -->
  <rect x="10" y="22" width="2" height="2" fill="#ffffff" opacity="0.3" rx="0.5"/>
  <rect x="36" y="22" width="2" height="2" fill="#ffffff" opacity="0.3" rx="0.5"/>
</svg>
